{
    "name": "goliatone/flatg",
    "description": "The simple flat file PHP [blog] engine.",
    "type": "library",
    "keywords": ["flatg", "flat-file", "flat", "blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "goliatone",
            "email": "hello@goliatone.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "goliatone\\flatg": "src/"
        },
        "include-path": ["mustangostang/spyc/Spyc.php"]
    },
    "minimum-stability": "dev",
    "require": {
    	"php": ">=5.4",
	    "suin/php-rss-writer": ">=1.2",
	    "buggedcom/customtags": "dev-master",
	    "mustangostang/spyc": "0.5.*@dev",
	    "benthedesigner/dropbox": "dev-master",
	    "csphere/githubwrapper": "*@dev",
	    "wolfie/php-markdown": "1.2.2@dev"
	},
    "require-dev": {
        "funkatron/funit": ">=0.3",
        "dotink/sage":"dev-master",
        "andrewsville/php-token-reflection": "1.3.1"
    },
    "repositories": [
	   {
	     "type": "git",
	     "url": "git@github.com:funkatron/FUnit.git"
	   },
	   {
            "type": "package",
            "package": {
                "name": "wolfie/php-markdown",
                "version": "1.2.2",
                "autoload": {"classmap": ["markdown.php"]},
                "source": {
                    "url": "git://github.com/wolfie/php-markdown.git",
                    "type": "git",
                    "reference": "extra"
                }
            }
        },
	    {
            "type": "package",
            "package": {
                "name": "csphere/githubwrapper",
                "version": "master",
                "autoload": {"classmap": ["githuboauth.class.php"]},
                "source": {
                    "url": "git://github.com/csphere/GithubApiWrapper-PHP.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
	    {
            "type": "package",
            "package": {
                "name": "dotink/sage",
                "version": "master",
                "autoload": {"classmap": ["src/"]},
                "source": {
                    "url": "git://github.com/dotink/sage.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }	   	
	],
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}